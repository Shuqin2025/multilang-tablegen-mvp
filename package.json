{
  "name": "tablegen-app",
  "version": "1.0.0",
  "private": true,
  "description": "Minimal Express backend + Vite frontend in one repo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",           // 后端照常启动
    "build": "vite build",               // 前端构建（Render 静态站会用到）
    "dev": "vite",                       // 本地开发可选
    "preview": "vite preview"            // 本地预览可选
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "18.x"
  }
}


